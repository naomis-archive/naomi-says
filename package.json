{
  "name": "naomi-says",
  "version": "1.0.0",
  "description": "A template for my nodejs projects",
  "main": "prod/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint src test --max-warnings 0 && prettier src test --check",
    "start": "node -r dotenv/config prod/index.js",
    "test": "ts-mocha -u tdd  test/**/*.spec.ts --recursive --exit --timeout 10000"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/naomi-lgbt/naomi-says.git"
  },
  "engines": {
    "node": "18.17.1",
    "pnpm": "8.6.12"
  },
  "keywords": [
    "template",
    "typescript",
    "eslint",
    "nodejs",
    "prettier"
  ],
  "author": "Naomi Carrigan",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/naomi-lgbt/naomi-says/issues"
  },
  "homepage": "https://github.com/naomi-lgbt/naomi-says#readme",
  "dependencies": {
    "discord.js": "14.13.0",
    "dotenv": "16.3.1",
    "node-html-to-image": "4.0.0",
    "winston": "3.10.0"
  },
  "devDependencies": {
    "@nhcarrigan/eslint-config": "1.1.3",
    "@nhcarrigan/prettier-config": "1.0.1",
    "@nhcarrigan/typescript-config": "1.0.1",
    "@types/chai": "4.3.5",
    "@types/mocha": "10.0.1",
    "@types/node": "18.17.5",
    "chai": "4.3.7",
    "eslint": "8.47.0",
    "mocha": "10.2.0",
    "prettier": "2.8.8",
    "ts-mocha": "10.0.0",
    "typescript": "5.1.6"
  }
}
